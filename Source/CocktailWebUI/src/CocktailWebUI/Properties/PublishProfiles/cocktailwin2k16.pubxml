<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>Custom</WebPublishMethod>
    <publishMethod>Docker</publishMethod>
    <profileName>cocktailwin2k16</profileName>
    <DockerAppType>Web</DockerAppType>
    <DockerServerUrl>tcp://cocktailwin2k16.eastus.cloudapp.azure.com:2376</DockerServerUrl>
    <DockerImageName>cocktailwebui</DockerImageName>
    <DockerPublishHostPort>8084</DockerPublishHostPort>
    <DockerPublishContainerPort>8084</DockerPublishContainerPort>
    <DockerBuildOnly>False</DockerBuildOnly>
    <DockerRemoveConflictingContainers>True</DockerRemoveConflictingContainers>
    <DockerAuthOptions>--tlsverify</DockerAuthOptions>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <PublishProvider>Docker</PublishProvider>
    <LastUsedBuildConfiguration />
    <LastUsedPlatform />
    <CompileSource>False</CompileSource>
    <UsePowerShell>True</UsePowerShell>
    <WebRoot>wwwroot</WebRoot>
    <WwwRootOut>wwwroot</WwwRootOut>
    <IncludeSymbols>False</IncludeSymbols>
    <Native>False</Native>
    <DockerfileRelativePath>..\..\Dockerfile</DockerfileRelativePath>
    <_DefaultDNXVersion>dnx-coreclr-win-x64.1.0.0-rc1-final</_DefaultDNXVersion>
    <IISCommand>web</IISCommand>
    <PublishPreviewUrl>tcp://cocktailwin2k16.eastus.cloudapp.azure.com:2376</PublishPreviewUrl>
    <CreateWindowsContainer>True</CreateWindowsContainer>
    <PublishDNXVersion>dnx-coreclr-win-x64.1.0.0-rc1-final</PublishDNXVersion>
  </PropertyGroup>
</Project>